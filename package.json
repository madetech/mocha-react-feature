{
  "name": "mocha-react-feature",
  "version": "0.2.0",
  "description": "Opinionated feature testing for React applications",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --require jsdom-global/register --reporter dist/reporter.js --compilers js:babel-register test/*.test.js",
    "build": "babel src/ -d dist/",
    "lint": "eslint -c .eslintrc src/",
    "prepublish": "npm run build"
  },
  "keywords": [
    "mocha",
    "react",
    "redux",
    "react-router-redux",
    "feature",
    "test"
  ],
  "author": "Richard Foster <richard@madetech.com> (http://www.madetech.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/madetech/mocha-react-feature"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.0",
    "jsdom-global": "^2.0.0",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2"
  },
  "peerDependencies": {
    "react-router-redux": "^4.0.0",
    "mocha": "^3.0.0"
  }
}
